<%
l1_price = parseFloat(l1_price);
l2_price = parseFloat(l2_price);
l3_price = parseFloat(l3_price);
l4_price = parseFloat(l4_price);
l5_price = parseFloat(l5_price);
var l1_gross = (l1_gross_ticket - l1_kill) * l1_price;
var l2_gross = (l2_gross_ticket - l2_kill) * l2_price;
var l3_gross = (l3_gross_ticket - l3_kill) * l3_price;
var l4_gross = (l4_gross_ticket - l4_kill) * l4_price;
var l5_gross = (l5_gross_ticket - l5_kill) * l5_price;

var sum_gross = l1_gross + l2_gross + l3_gross + l4_gross + l5_gross;
var sum_gross_ticket = l1_gross_ticket + l2_gross_ticket + l3_gross_ticket + l4_gross_ticket + l5_gross_ticket;
var sum_kill = l1_kill + l2_kill + l3_kill + l4_kill + l5_kill;
var playable_on = new Date(show_date);
switch (post_show_lockout_unit) {
	case 'Days':
		playable_on = new Date(playable_on.setTime(playable_on.getTime() + post_show_lockout * 86400000));
		break;
	case 'Months':
		playable_on = new Date(playable_on.setMonth(playable_on.getMonth() + post_show_lockout));
		break;
	default:
		break;
}
var support_artist_total = parseFloat(support_artist_1_total) + parseFloat(support_artist_2_total) + parseFloat(support_artist_3_total);

var tour_production_expenses = app.utils.misc.calc_sum_from_array(JSON.parse(production_expense));
var general_expenses_total = app.utils.misc.calc_sum_from_array(JSON.parse(general_expense));
var total_estimated_expenses = tour_production_expenses + general_expenses_total;

var ve = JSON.parse(variable_expense);
var total_variable_expense = 999;
%>

