				<div class="row">
					<label class="col-xs-3">On Sale Date<input name="on_sale_date" type="date"

															   class="form-control">
					</label>
					<label class="col-xs-1">TBD <br><input type="checkbox"

						>
					</label>
					<label class="col-xs-5">Ticketing Company<select name="ticketing_company_id"

																	 class="form-control">
							<% _.each(app.collections.ticketing_companies.models, function (a_company) { %>
							<option value="<%= a_company.get('id') %>"><%= a_company.get('name') %></option>
							<% }); %>
						</select>
					</label>
					<label class="col-xs-3">On Sale Date<input type="date"

															   class="form-control">
					</label>
				</div>
