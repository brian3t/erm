<div class="col-sm-2 col-xs-12">
	<button type="button" class="btn btn-sm btn-primary toggle_create_mode create"><span class="glyphicon glyphicon-plus"></span> Create new Offer
	</button>
	<div class="clearfix"></div>
	<br>
	<input type="search" id="offer_search" class="col-xs-12" name="offer_search" placeholder="Filter offers">
	<div id="offer_search_list" class="col-xs-12 padded search_list">
		<br/>
	</div>
</div>
<div class="col-sm-10 col-xs-12 form_wrapper">
	<div class="action_buttons">
		<label><input class="edit_switch" type="checkbox">&nbsp;Edit mode is <span class="toggle_state">on</span></label>
	</div>
	<hr>
	<div id="offer_form_wrapper" class="col-xs-12 edit_form_wrapper">
	</div>
	<div id="create_offer" style="display: none">
		<form class="account_info create" action="#" data-model="offer" data-toggle="validator" data-disable=true
			  role="form">

			<div class="form-group">
				<div class="row">
					<label class="col-xs-6">Offer Event ID<input name="event_id" type="text"

																 class="form-control">
					</label>
					<label class="col-xs-6">Show# <br/><input name="show_number" type="text"

						> of <input name="show_total_num" type="text"

						>
					</label>
				</div>
				<div class="row">
					<label class="col-xs-4">Agency<select name="agency_id" type="text"

														  class="form-control">
							<% _.each(app.collections.companies.models, function (a_company) { %>
							<option value="<%= a_company.get('id') %>"><%= a_company.get('name') %></option>
							<% }); %>
						</select>
					</label>

					<label class="col-xs-4">Agent<select name="agent_id" type="text"

														 class="form-control">
							<% _.each(app.collections.agents.models, function (an_agent) { %>
							<option value="<%= an_agent.get('id') %>"><%= an_agent.get('name') %></option>
							<% }); %>
						</select>
					</label>
					<label class="col-xs-4">Status<select name="status"

														  class="form-control">
							<option value="New" selected>New</option>
							<option value="Submitted">Submitted</option>
							<option value="Pending">Pending</option>
							<option value="Reviewed">Reviewed</option>
							<option value="Declined">Declined</option>
							<option value="Confirmed">Confirmed</option>
							<option value="Revised">Revised</option>
							<option value="Signed">Signed</option>
							<option value="Unsubmitted">Unsubmitted</option>
						</select>
					</label>

				</div>

				<div class="row">
					<label class="col-xs-6">Booked By<input name="user_id" hidden type="text"
															value="<%= app.cur_user.get('id') %>">
						<input readonly type="text"
							   value="<%= app.cur_user.getFullName() %>"
							   class="form-control">
					</label>
					<label class="col-xs-6">Offer Type<select name="offer_type" type="text"

															  class="form-control">
							<option></option>
							<option>Icon</option>
							<option>Co-Pro Promoter</option>
							<option>Co-Pro Venue</option>
							<option>3rd Party</option>
						</select>
					</label>
				</div>
				<div class="row">
					<label class="col-xs-6">Co-Pro Promoter<select name="copro_promoter_id"

																   class="form-control">
							<% _.each(app.collections.promoters.models, function (a_promoter) { %>
							<option value="<%= a_promoter.get('id') %>"><%= a_promoter.get('name') %></option>
							<% }); %>
						</select>
					</label>
					<label class="col-xs-6">Co-Pro Venue<select name="copro_venue_id"

																class="form-control">
							<% _.each(app.collections.venues.models, function (a_venue) { %>
							<option value="<%= a_venue.get('id') %>"><%= a_venue.get('name') %></option>
							<% }); %>
						</select>
					</label>
				</div>
			</div>
			<h4>General Information</h4>
			<div class="form-group">
				<div class="row">
					<label class="col-xs-6">Artist<select name="artist_id"

														  class="form-control">
							<% _.each(app.collections.artists.models, function (an_artist) { %>
							<option value="<%= an_artist.get('id') %>"><%= an_artist.getFullName() %></option>
							<% }); %>
						</select>
					</label>
					<label class="col-xs-6">Venue<select name="venue_id"

														 class="form-control">
							<% _.each(app.collections.venues.models, function (a_venue) { %>
							<option value="<%= a_venue.get('id') %>"><%= a_venue.get('name') %></option>
							<% }); %>
						</select>
					</label>
				</div>
				<div class="row">
					<label class="col-xs-6">Show Date<input name="show_date" type="date"

															class="form-control">
					</label>
					<label class="col-xs-6">Show Date TBD<br><input name="is_tbd_date" type="checkbox"
						>
					</label>
				</div>
				<div class="row">
					<label class="col-xs-4">Show Type<select name="show_type"

															 class="form-control">
							<option></option>
							<option>Private</option>
							<option>Public</option>
							<option>College</option>
							<option>College - Private</option>
							<option>Casino</option>
							<option>Fair</option>
							<option>Festival</option>
						</select>
					</label>
					<label class="col-xs-4">Doors<br><input name="doors" type="time"
															class="form-control">
					</label>
					<label class="col-xs-4">Showtime<br><input name="showtime" type="time"
															   class="form-control">
					</label>
				</div>
				<div class="row">
					<label class="col-xs-12">Notes<textarea name="notes"

															class="form-control"></textarea>
					</label>
				</div>
			</div>
			<h4>Ticket Scaling</h4>
